{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": ["es5", "scripthost", "es2015", "dom"],
    "importHelpers": true,
    "strict": true,
    "skipLibCheck": true,
    "pretty": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noErrorTruncation": true
  }
}
